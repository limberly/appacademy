<h1><%=@cat.name%></h1>

<% @cat.toys.each do |toy| %>
  <li>
    <%= toy.name%>, <%= toy.ttype%>
  </li>
<% end %>

<ul>
  <li>
    <a href="<%=new_cat_toy_url(@cat)%>">New Toy</a>
  </li>

  <li>
    <a href="<%=edit_cat_url(@cat)%>">Edit Cat</a>
  </li>

  <li>
    <a href="<%=cats_url%>">Index</a>
  </li>

</ul>